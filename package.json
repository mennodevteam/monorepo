  "scripts": {
    "deploy-panel": "nx run panel:build --skip-nx-cache; liara deploy --path=dist/apps/panel",
    "deploy-admin": "nx run admin:build --skip-nx-cache; liara deploy --path=dist/apps/admin",
    "deploy-app": "nx run app:build --skip-nx-cache; liara deploy --path=dist/apps/app",
    "deploy-new-app": "nx run new-app:build --prod --skip-nx-cache; liara deploy --path=dist/apps/new-app",
    "deploy-backend": "nx run backend:build --skip-nx-cache; liara deploy --path=dist/apps/backend",
    "build": "nx run-many --target=build --all --maxParallel=100",
    "dev": "nx run-many --target=serve --all --maxParallel=100",
    "dev:panel": "nx run panel:serve",
    "dev:admin": "nx run admin:serve",
    "dev:app": "nx run app:serve --host 0.0.0.0",
    "dev:new-app": "nx run new-app:serve --host 0.0.0.0",
    "dev:backend": "nx run backend:serve",
    "dev:new-panel": "nx run new-panel:serve"
  },
