{
  "appData": {
    "forceReload": true,
    "lazyReload": false,
    "changes": [
      "گزارش روزانه فروش",
      "گزارش پیامک‌های ارسالی به صورت گروه بندی",
      "بهبود رابط کاربری در موبایل",
      "امکان محدود کردن تعداد استفاده از کد تخفیف"
    ]
  },
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ]
}
