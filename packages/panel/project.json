{
  "name": "panel",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/panel/src",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/panel",
        "index": "packages/panel/src/index.html",
        "main": "packages/panel/src/main.ts",
        "polyfills": "packages/panel/src/polyfills.ts",
        "tsConfig": "packages/panel/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["packages/panel/src/favicon.ico", "packages/panel/src/assets"],
        "styles": ["packages/panel/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": [
            "packages/panel/src/theme/"
          ]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "packages/panel/src/environments/environment.ts",
              "with": "packages/panel/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "panel:build:production"
        },
        "development": {
          "browserTarget": "panel:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "panel:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/panel/**/*.ts", "packages/panel/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/panel/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
