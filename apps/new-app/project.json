{
  "name": "new-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/new-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/new-app",
        "index": "apps/new-app/src/index.html",
        "main": "apps/new-app/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/new-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/new-app/public"
          }
        ],
        "styles": [
          "apps/new-app/src/themes/_base.scss",
          "apps/new-app/src/styles.scss",
          {
            "input": "apps/new-app/src/themes/red-theme.scss",
            "inject": false,
            "bundleName": "red-theme"
          },
          {
            "input": "apps/new-app/src/themes/green-theme.scss",
            "inject": false,
            "bundleName": "green-theme"
          },
          {
            "input": "apps/new-app/src/themes/blue-theme.scss",
            "inject": false,
            "bundleName": "blue-theme"
          },
          {
            "input": "apps/new-app/src/themes/yellow-theme.scss",
            "inject": false,
            "bundleName": "yellow-theme"
          },
          {
            "input": "apps/new-app/src/themes/cyan-theme.scss",
            "inject": false,
            "bundleName": "cyan-theme"
          },
          {
            "input": "apps/new-app/src/themes/magenta-theme.scss",
            "inject": false,
            "bundleName": "magenta-theme"
          },
          {
            "input": "apps/new-app/src/themes/orange-theme.scss",
            "inject": false,
            "bundleName": "orange-theme"
          },
          {
            "input": "apps/new-app/src/themes/chartreuse-theme.scss",
            "inject": false,
            "bundleName": "chartreuse-theme"
          },
          {
            "input": "apps/new-app/src/themes/spring-green-theme.scss",
            "inject": false,
            "bundleName": "spring-green-theme"
          },
          {
            "input": "apps/new-app/src/themes/azure-theme.scss",
            "inject": false,
            "bundleName": "azure-theme"
          },
          {
            "input": "apps/new-app/src/themes/violet-theme.scss",
            "inject": false,
            "bundleName": "violet-theme"
          },
          {
            "input": "apps/new-app/src/themes/rose-theme.scss",
            "inject": false,
            "bundleName": "rose-theme"
          },
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/new-app/src/themes/"]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/new-app/src/environments/environment.ts",
              "with": "apps/new-app/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "new-app:build:production"
        },
        "development": {
          "buildTarget": "new-app:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "new-app:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/new-app/jest.config.ts"
      }
    }
  }
}
